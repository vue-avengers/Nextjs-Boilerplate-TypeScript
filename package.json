{
  "name": "hangikredi-fe-next",
  "version": "1.0.0",
  "description": "Kredi hesaplama, banka karşılaştırma ve kart başvuru işlemlerini hızlıca yap; kasko ve sigorta tekliflerini hangikredi.com ile hemen gör!",
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "yarn build && next start",
    "analyze": "rimraf .next/analyze && cross-env ANALYZE=true yarn build",
    "export": "cross-env NODE_ENV=production next export",
    "build-prod": "run-s clean build export",
    "clean": "rimraf .next out",
    "lint": "next lint",
    "lint:fix": "yarn lint --fix",
    "stylelint": "stylelint ./src/**/*.{css,scss}",
    "stylelint:fix": "stylelint --fix ./src/**/*.{css,scss}",
    "pretty": "prettier --write --config .prettierrc.json {src,e2e,docs}/**/*.{ts,tsx}",
    "build-types": "tsc --noEmit --pretty",
    "prepare": "husky install",
    "test": "yarn test:unit && yarn test:e2e",
    "test:unit": "jest",
    "test:watch": "jest --watchAll --verbose",
    "test:unit:coverage": "jest --coverage",
    "test:unit:update": "jest --updateSnapshot",
    "test:e2e": "cypress run --config video=false --project ./e2e",
    "test:e2e:ui": "cypress open --project ./e2e"
  },
  "dependencies": {
    "next": "^12.0.7",
    "next-compose-plugins": "^2.2.1",
    "next-pwa": "^5.4.4",
    "next-rosetta": "^1.3.1",
    "next-seo": "^4.28.1",
    "preact": "^10.6.2",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "sass": "^1.45.0",
    "styled-jsx-plugin-postcss": "^4.0.1"
  },
  "devDependencies": {
    "@next/bundle-analyzer": "^12.0.2",
    "@testing-library/jest-dom": "^5.16.1",
    "@testing-library/react": "^12.1.2",
    "@types/node": "^16.11.6",
    "@types/react": "^17.0.33",
    "@typescript-eslint/eslint-plugin": "^4.33.0",
    "@typescript-eslint/parser": "^4.33.0",
    "autoprefixer": "^10.2.6",
    "cross-env": "^7.0.3",
    "cypress": "^9.1.1",
    "esbuild-loader": "^2.16.0",
    "eslint": "^7.32.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-config-airbnb-typescript": "^14.0.1",
    "eslint-config-next": "^12.0.2",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.25.2",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.26.1",
    "eslint-plugin-react-hooks": "^4.2.0",
    "eslint-plugin-unused-imports": "^1.1.5",
    "husky": "^7.0.4",
    "jest": "^27.4.3",
    "lint-staged": "^11.2.6",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.4",
    "prettier": "^2.4.1",
    "react-test-renderer": "^17.0.2",
    "rimraf": "^3.0.2",
    "stylelint": "^14.1.0",
    "stylelint-config-recommended-scss": "^5.0.2",
    "stylelint-order": "^5.0.0",
    "stylelint-scss": "^4.0.1",
    "tailwindcss": "^3.0.0",
    "typescript": "^4.4.4"
  }
}
